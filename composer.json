{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/passport": "^1.0",
        "league/fractal": "^0.14.0",
        "barryvdh/laravel-cors": "^0.8.2",
        "bosnadev/repositories": " 0.*",
        "paquettg/php-html-parser": "^1.7",
        "laravel/tinker": "^1.0",
        "jenssegers/agent": "^2.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "viender/database"
        ],
        "psr-4": {
            "App\\": "viender/app/",
            "Viender\\Socialite\\": "apps/socialite/src/",
            "Viender\\Profile\\": "apps/profile/src/",
            "Viender\\Dealer\\": "apps/dealer/src/",
            "Viender\\Utilities\\": "apps/utilities/src/",
            "Viender\\Mytutor\\": "apps/mytutor/src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "viender/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php manage key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php manage optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php manage optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "vendor-dir": "viender/vendor"
    }
}
